    "def get_min_number(numbers):\n",
    "    result = numbers[0]\n",
    "    for num in numbers:\n",
    "        if result > num:\n",
    "            result = num\n",
    "    return result\n",
    "\n",
    "min_number = get_min_number(numbers)\n",
    "print(\"min number is: \",min_number)\n",
    "\n",
    "numbers = [56,2,13,-8,0,8]"
   ]
